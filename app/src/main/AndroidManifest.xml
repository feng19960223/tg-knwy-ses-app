<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.turingoal.bjcorona.ses">
    <!-- 访问权限-访问网络连接 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 访问权限-获取网络信息状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 访问权限-WIFI状态 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 访问权限-WIFI变化 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <!-- 访问权限-设备信息 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 访问权限-拨打电话 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:name=".app.TgApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- 应用启动页 -->
        <activity
            android:name=".ui.activity.common.TgWelcomeActivity"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 主页面 -->
        <activity
            android:name=".ui.activity.common.TgMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 关于页面 -->
        <activity
            android:name=".ui.activity.common.TgAboutActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 参数-电网 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ParameterPowerGridActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 参数-电网 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ParameterInverterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 参数-负载 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ParameterLoadActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 参数-电池 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ParameterBarrteryActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 参数-面板 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ParameterPanelActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 配置-登录 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ConfigLoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 配置-最后设置 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ConfigLastSetupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 配置-工作模式 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ConfigWorkPatternActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 配置-工作模式-经济模式 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ConfigWorkPatternEconomicActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
        <!-- 配置-IP配置 -->
        <activity
            android:name="com.turingoal.bjcorona.ses.ui.activity.ConfigIpSetupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme" />
    </application>
</manifest>